                                    <td>Munich</td>
                                    <td>Allianz Arena</td>
                                    <td>26</td>
                                    <td>Bundesliga</td>
                                    <td>1900</td>
                                </tr>
                                <tr>
                                    <td>4</td>
                                    <td>Juventus FC</td>
                                    <td>Turin</td>
                                    <td>Allianz Stadium</td>
                                    <td>24</td>
                                    <td>Serie A</td>
                                    <td>1897</td>
                                </tr>
                                <tr>
                                    <td>5</td>
                                    <td>Paris Saint-Germain</td>
                                    <td>Paris</td>
                                    <td>Parc des Princes</td>
                                    <td>23</td>
                                    <td>Ligue 1</td>
                                    <td>1970</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            
            <!-- Standings End -->


            <!-- Widgets Start -->
            <div class="container-fluid pt-4 px-4">
                <div class="row g-4">
                    <div class="col-sm-12 col-md-6 col-xl-4">
                        <div class="h-100 bg-light rounded p-4">
